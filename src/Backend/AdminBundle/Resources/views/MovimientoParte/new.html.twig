{% extends '::backend.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
     {% stylesheets '@BackendAdminBundle/Resources/public/css/ingresos_partes.css' %}
     <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}

{% endblock %}


{% block container -%}
    <h3>Nuevo Movimiento de Partes</h3>
    
   
    <div class="well">
    <div id="myTabContent" class="tab-content">
    <div class="tab-pane active in" id="home">
    <form class="form-horizontal" id="tab" name="tab" method="post" novalidate="true"    action="{{ path('movimientoParte_create') }}" >
     {{ form_errors(form) }}
   

    
    {% include 'BackendAdminBundle:MovimientoParte:_movimientoParte.html.twig' %} 

    
    {{ form_rest(form) }}

    <div class="control-group">
    <!-- Button -->
    <div class="controls">
    <button id="agregar" type="button" class="btn" data-url="{{ path('movimientoParte_toprocesadomovimiento') }}">Agregar partes</button>
    <button id="terminar" type="button" class="btn" data-url="{{ path('movimientoParte_toclosemovimiento') }}" data-redirect="{{ path('movimientoParte') }}"><i class="icon-chevron-left icon-white"></i> Terminar</button>
    
    </div>
    
    </div>
    </form>
    </div>
    
     </div>
      </div>
    

       
{% endblock %}
{% block javascripts %}
  {{ parent() }}
{% javascripts '@BackendAdminBundle/Resources/public/js/move_partes.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
 
{% javascripts '@BackendAdminBundle/Resources/public/js/validate_movimiento_partes.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
